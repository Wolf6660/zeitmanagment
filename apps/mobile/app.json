{
  "expo": {
    "name": "Zeitmanagment Beta",
    "slug": "zeitmanagment-mobile",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "de.zeitmanagment.mobile",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "Die Kamera wird nur genutzt, um den verschluesselten Einrichtungs-QR-Code zu scannen."
      }
    },
    "android": {
      "package": "de.zeitmanagment.mobile",
      "versionCode": 1
    },
    "plugins": [
      "expo-secure-store",
      "expo-camera"
    ],
    "extra": {
      "releaseChannel": "beta"
    }
  }
}
